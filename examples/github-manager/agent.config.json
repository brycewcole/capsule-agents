{
  "agent": {
    "name": "Github Manager",
    "description": "An agent that manages GitHub repositories, issues, and pull requests.",
    "model": {
      "name": "openai/gpt-5-mini"
    },
    "tools": {
      "web_search": {
        "enabled": true
      }
    }
  },
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "Github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GITHUB_PAT}"
      }
    }
  }
}